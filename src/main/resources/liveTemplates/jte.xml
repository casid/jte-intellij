<templateSet group="jte">
    <template name="@import" value="@import $END$" description="jte import" toReformat="false" toShortenFQNames="false">
        <context><option name="jte" value="true"/></context>
    </template>
    <template name="@param" value="@param $END$" description="jte param" toReformat="false" toShortenFQNames="false">
        <context><option name="jte" value="true"/></context>
    </template>
    <template name="@if" value="@if($END$)&#10;@endif" description="jte if statement" toReformat="false" toShortenFQNames="false">
        <context><option name="jte" value="true"/></context>
    </template>
    <template name="@elseif" value="@elseif($END$)" description="jte elseif statement" toReformat="false" toShortenFQNames="false">
        <context><option name="jte" value="true"/></context>
    </template>
    <template name="@else" value="@else$END$" description="jte else statement" toReformat="false" toShortenFQNames="false">
        <context><option name="jte" value="true"/></context>
    </template>
    <template name="@for" value="@for($END$)&#10;@endfor" description="jte for statement" toReformat="false" toShortenFQNames="false">
        <context><option name="jte" value="true"/></context>
    </template>

    <template name="${" value="${$END$}" description="jte output" toReformat="false" toShortenFQNames="false">
        <context><option name="jte" value="true"/></context>
    </template>

    <template name="!{var" value="!{var $NAME$ = $END$;}" description="jte var" toReformat="false" toShortenFQNames="false">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
        <context><option name="jte" value="true"/></context>
    </template>

    <template name="@raw" value="@raw$END$@endraw" description="jte raw content" toReformat="false" toShortenFQNames="false">
        <context><option name="jte" value="true"/></context>
    </template>
</templateSet>
